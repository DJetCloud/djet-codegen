<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
		xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">

	<property name="TEST_VARCHAR" value="varchar(4096)"/>
	<property name="UUID" value="varchar(36)" dbms="{{database.name}},h2"/>
	<property name="JSON_OBJECT" value="json" dbms="{{database.name}}"/>
	<property name="JSON_OBJECT" value="${TEST_VARCHAR}" dbms="h2"/>
	<property name="BOOLEAN_VALUE" value="boolean" dbms="{{database.name}},h2"/>
	{{#isMySQL}}
	<property name="TINYBLOB_OBJECT" value="tinyblob" dbms="mysql"/>
	<property name="TINYBLOB_OBJECT" value="${TEST_VARCHAR}" dbms="h2"/>
	<property name="TEXT_OBJECT" value="text" dbms="mysql"/>
	<property name="TEXT_OBJECT" value="${TEST_VARCHAR}" dbms="h2"/>
	<property name="TINYTEXT_OBJECT" value="tinytext" dbms="mysql"/>
	<property name="TINYTEXT_OBJECT" value="${TEST_VARCHAR}" dbms="h2"/>
	<property name="BLOB_OBJECT" value="blob" dbms="mysql"/>
	<property name="BLOB_OBJECT" value="${TEST_VARCHAR}" dbms="h2"/>
	<property name="MEDIUMBLOB_OBJECT" value="mediumblob" dbms="mysql"/>
	<property name="MEDIUMBLOB_OBJECT" value="${TEST_VARCHAR}" dbms="h2"/>
	<property name="MEDIUMTEXT_OBJECT" value="mediumtext" dbms="mysql"/>
	<property name="MEDIUMTEXT_OBJECT" value="${TEST_VARCHAR}" dbms="h2"/>
	<property name="LONGBLOB_OBJECT" value="longblob" dbms="mysql"/>
	<property name="LONGBLOB_OBJECT" value="${TEST_VARCHAR}" dbms="h2"/>
	<property name="LONGTEXT_OBJECT" value="longtext" dbms="mysql"/>
	<property name="LONGTEXT_OBJECT" value="${TEST_VARCHAR}" dbms="h2"/>
	{{/isMySQL}}
	{{#isPostgreSQL}}
	<property name="VARCHAR_OBJECT" value="text" dbms="postgresql"/>
	<property name="VARCHAR_OBJECT" value="${TEST_VARCHAR}" dbms="h2"/>
	<property name="TEXT_OBJECT" value="text" dbms="postgresql"/>
	<property name="TEXT_OBJECT" value="${TEST_VARCHAR}" dbms="h2"/>
	{{/isPostgreSQL}}
	<property name="TEXT_TYPE" value="text" dbms="{{database.name}}"/>
	<property name="TEXT_TYPE" value="longvarchar" dbms="h2"/>
</databaseChangeLog>